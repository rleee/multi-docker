{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
     {
       "name": "client",
       "image": "danlee0150/multi-client",
       "hostname": "client",
       "essential": false
     },
     {
      "name": "server",
      "image": "danlee0150/multi-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "danlee0150/multi-worker",
      "hostname": "worker",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "danlee0150/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"]
    }
  ]
}